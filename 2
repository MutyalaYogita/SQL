SELECT * 
FROM student
WHERE major = 'Biology' OR name = 'Mike' OR major = 'Chemistry';

SELECT * 
FROM student
WHERE student_id <=3 AND name <> 'Jack';

SELECT * 
FROM student
WHERE major IN ('Biology', 'Chemistry');

SELECT *
FROM student
ORDER BY major, student_id ; -- alphabetical

SELECT * 
FROM student
ORDER BY student_id DESC
LIMIT 1; -- gives the most recently addes student
